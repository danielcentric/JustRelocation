            renderVendors() {
                // Group vendors by category
                const vendorsByCategory = {};
                this.vendors.forEach(vendor => {
                    if (!vendorsByCategory[vendor.category]) {
                        vendorsByCategory[vendor.category] = [];
                    }
                    vendorsByCategory[vendor.category].push(vendor);
                });

                // Render each category
                Object.keys(vendorsByCategory).forEach(category => {
                    const containerId = this.getCategoryContainerId(category);
                    const container = document.getElementById(containerId);
                    if (container) {
                        container.innerHTML = vendorsByCategory[category].map(vendor => this.createVendorCard(vendor)).join('');
                    }
                });
            }

            getCategoryContainerId(category) {
                switch (category) {
                    case 'Banking': return 'bankingVendors';
                    case 'Housing': return 'housingVendors';
                    case 'Restaurants': return 'restaurantVendors';
                    case 'Hotels': return 'hotelVendors';
                    case 'GP Practices':
                    case 'Pharmacies': return 'healthcareVendors';
                    case 'Supermarkets':
                    case 'Gyms': return 'essentialVendors';
                    case 'Transport': return 'transportVendors';
                    default: return 'essentialVendors';
                }
            }

            createVendorCard(vendor) {
                const features = vendor.specializations ? vendor.specializations.split(', ') : [];

                return `
                    <div class="vendor-card">
                        <div class="vendor-image">
                            <img src="${vendor.image || 'https://images.unsplash.com/photo-1486406146926-c627a92ad1ab?w=400&h=250&fit=crop&crop=center'}"
                                 alt="${vendor.business_name}" loading="lazy">
                            <div class="vendor-distance">${vendor.distance_from_hospital}km</div>
                        </div>
                        <div class="vendor-content">
                            <div class="vendor-header">
                                <div>
                                    <h3 class="vendor-name">${vendor.business_name}</h3>
                                    <span class="vendor-category-tag">${vendor.subcategory || vendor.category}</span>
                                </div>
                            </div>

                        <div class="vendor-address">
                            <i class="fas fa-map-marker-alt"></i>
                            ${vendor.address}, ${vendor.postcode}
                        </div>

                        <div class="vendor-hours">
                            <i class="fas fa-clock"></i>
                            <strong>Hours:</strong> ${vendor.business_hours}
                        </div>

                        <div class="vendor-rating">
                            <div class="rating-stars">
                                ${this.generateStars(vendor.rating)}
                            </div>
                            <span>${vendor.rating}/5.0 (${vendor.review_count} reviews)</span>
                        </div>

                        <div class="vendor-features">
                            ${features.slice(0, 3).map(feature => `<span class="feature-tag">${feature}</span>`).join('')}
                        </div>

                        <div class="vendor-actions">
                            <a href="tel:${vendor.phone}" class="action-btn btn-primary">
                                <i class="fas fa-phone"></i>
                                Call Now
                            </a>
                            ${vendor.website ? `
                                <a href="${vendor.website}" target="_blank" class="action-btn btn-outline">
                                    <i class="fas fa-globe"></i>
                                    Visit Site
                                </a>
                            ` : ''}
                            <button class="action-btn btn-success" onclick="this.showVendorDetails('${vendor.business_name}')">
                                <i class="fas fa-info-circle"></i>
                                Details
                            </button>
                            </div>
                        </div>
                    </div>
                `;
            }

            generateStars(rating) {
                const fullStars = Math.floor(rating);
                const halfStar = rating % 1 >= 0.5;
                const emptyStars = 5 - fullStars - (halfStar ? 1 : 0);

                return '‚òÖ'.repeat(fullStars) +
                       (halfStar ? '‚òÜ' : '') +
                       '‚òÜ'.repeat(emptyStars);
            }

            showVendorDetails(vendorName) {
                const vendor = this.vendors.find(v => v.business_name === vendorName);
                if (vendor) {
                    alert(`${vendor.business_name}\n\nServices: ${vendor.services_offered}\n\nSpecializations: ${vendor.specializations}\n\nPhone: ${vendor.phone}`);
                }
            }
        }

        // WhatsApp Support
        function openWhatsAppSupport() {
            const message = "Hi! I need help finding services near Guy's Hospital through JustRelocation.";
            const whatsappUrl = `https://wa.me/447700900900?text=${encodeURIComponent(message)}`;
            window.open(whatsappUrl, '_blank');
        }

        // Account Menu Dropdown
        function toggleAccountDropdown() {
            const dropdown = document.getElementById('accountDropdown');
            if (dropdown) {
                dropdown.classList.toggle('show');
            }
        }

        // Close dropdown when clicking outside
        document.addEventListener('click', function(event) {
            const accountMenu = document.querySelector('.account-menu');
            const dropdown = document.getElementById('accountDropdown');

            if (accountMenu && dropdown && !accountMenu.contains(event.target)) {
                dropdown.classList.remove('show');
            }
        });

        // Logout function
        function logout() {
            alert('Logging out...');
        }

        // Initialize when DOM is loaded
        document.addEventListener('DOMContentLoaded', () => {
            window.guysHospital = new GuysHospitalDirectory();
        });
    </script>
