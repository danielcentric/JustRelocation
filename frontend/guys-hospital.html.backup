    <!-- Guy's Hospital JavaScript -->
    <script>
        class GuysHospitalDirectory {
            constructor() {
                this.vendors = [];
                this.loadVendorData();
            }

            loadVendorData() {
                // Load vendors with real images
                this.vendors = [
                    // Banking
                    {
                        category: 'Banking',
                        business_name: 'HSBC Bank',
                        subcategory: 'High Street Bank',
                        address: '28 Borough High Street, Southwark, London',
                        postcode: 'SE1 1YB',
                        phone: '0345 740 4404',
                        website: 'https://www.hsbc.co.uk',
                        distance_from_hospital: 0.35,
                        business_hours: 'Mon-Fri: 09:30-16:30, Sat: 09:30-13:00, Sun: Closed',
                        services_offered: 'Counter service, ATM, coin deposit/withdrawal, balance enquiry, Wi-Fi, NHS banking packages',
                        rating: 3.8,
